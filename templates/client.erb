# This file is managed by Puppet class <%= @name %>

Client {
  Name          = <%= @name %>
  Address       = <%= @address %>
  FDPort        = <%= @fdport %>
  Catalog       = <%= @catalog %>
  Password      = <%= @password %>
  FileRetention = <%= @file_retention %>
  JobRetention  = <%= @job_retention %>
  AutoPrune     = <%= @auto_prune %>
  <%- @config_options.sort.each do |key, value| -%>
      <%= key %> = <%= value %>
  <%- end -%>
}
